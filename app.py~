from flask import Flask, request
from flask_restful import Api, Resource
from flask_sqlalchemy import SQLAlchemy


app = Flask(__name__)
BASE = "https://jsonplaceholder.typicode.com"

@app.route('/')
def index():
    return 'Hello!'

@app.route('/posts')
def posts():
    """
    lists all posts
    """
    response = request.get(BASE + "/posts/1").json()
    print(response)

